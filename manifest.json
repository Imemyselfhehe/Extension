{
	"name": "dom",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"permissions": ["storage","tabs","background","downloads"],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	}
} 
